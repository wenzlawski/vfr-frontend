{
  "name": "readanalytics",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:end": "playwright test",
    "coverage": "vitest run --coverage",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "commit": "git add . && cz",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/cz-commitlint": "^17.4.4",
    "@fontsource/fira-mono": "^4.5.10",
    "@neoconfetti/svelte": "^1.0.0",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-node": "^1.0.0",
    "@sveltejs/kit": "^1.10.0",
    "@sveltejs/vite-plugin-svelte": "^2.0.3",
    "@testing-library/svelte": "^3.2.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/cookie": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitest/ui": "^0.29.2",
    "autoprefixer": "^10.4.13",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.3",
    "inquirer": "^9.1.4",
    "jsdom": "^21.1.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "sass": "^1.58.3",
    "svelte": "^3.55.1",
    "svelte-check": "^2.9.2",
    "svelte-hero-icons": "^4.1.3",
    "svelte-loader": "^3.1.6",
    "tailwindcss": "^3.2.6",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-top-level-await": "^1.3.0",
    "vitest": "^0.25.3"
  },
  "type": "module",
  "dependencies": {
    "@auth/core": "^0.5.1",
    "@auth/sveltekit": "^0.3.0",
    "@extractus/article-extractor": "^7.2.10",
    "@felte/reporter-dom": "^1.1.5",
    "@felte/validator-zod": "^1.0.13",
    "@next-auth/mongodb-adapter": "^1.1.1",
    "@sveltejs/adapter-vercel": "^2.3.0",
    "@tiptap/core": "2.0.0-beta.220",
    "@tiptap/pm": "2.0.0-beta.220",
    "@tiptap/starter-kit": "2.0.0-beta.220",
    "@vercel/analytics": "^0.1.11",
    "bcrypt": "^5.1.0",
    "daisyui": "^2.50.1",
    "felte": "^1.2.7",
    "mongodb": "^5.1.0",
    "mongoose": "^7.0.1",
    "mrm": "^4.1.13",
    "object-to-formdata": "^4.4.2",
    "svelte-disable-preload": "^0.0.1",
    "svelte-french-toast": "^1.0.3",
    "svelte-preprocess": "^5.0.1",
    "svelte-splitpanes": "^0.7.12",
    "zod": "^3.20.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,css,md,scss,html,json,jsx,svelte,ts}": "prettier --write"
  }
}
